<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">

		<title>
			{% block title %}Ink & Frame Studios{% endblock %}
		</title>
		<meta name="description" content="{% block meta_description %}Default meta description.{% endblock %}">
		<meta name="keywords" content="{% block meta_keywords %}default, keywords{% endblock %}">
		<meta name="author" content="{% block meta_author %}Default Author{% endblock %}">
		{% block meta_tags %}{% endblock %}
		
		
		<meta name="robots" content="index, follow">


		<link rel="icon" href="{{ asset('images/logo.png') }}">
		{% block stylesheets %}
			<link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css"/>

			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			<script src="https://unpkg.com/aos@next/dist/aos.js"></script>

		{% endblock %}
	</head>
	<body>
		{% include 'header.html.twig' %}
		{% block body %}{% endblock %}
		{% include 'footer.html.twig' %}
	</body>
</html>
